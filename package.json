{
  "name": "easy-qr",
  "version": "2.0.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "chart.js": "^3.4.1",
    "qr-code-styling": "^1.6.0-rc.1",
    "react": "^17.0.2",
